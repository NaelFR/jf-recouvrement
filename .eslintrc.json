{
  "extends": ["next/core-web-vitals","eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "prototypejs": true
  },
  "rules": {
    "indent": [
      "error",
      4
    ]
  }
}